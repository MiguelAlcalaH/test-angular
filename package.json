{
  "name": "mig-angular",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start:en": "ng serve --proxy-config proxy.conf.js --configuration=en",
    "start:es": "ng serve --proxy-config proxy.conf.js --configuration=es",
    "lint": "ng lint && stylelint \"src/**/*.scss\"",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --configuration=ci",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate",
    "extract:i18n": "ng extract-i18n --output-path src/locale"
  },
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/localize": "^17.3.12",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@azure/msal-angular": "^4.0.2",
    "@azure/msal-browser": "^4.0.2",
    "material-design-icons-iconfont": "^6.1.0",
    "moment": "^2.29.3",
    "rxjs": "^7.5.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.11",
    "@angular-eslint/builder": "~18.4.3",
    "@angular-eslint/eslint-plugin": "~18.4.3",
    "@angular-eslint/eslint-plugin-template": "~18.4.3",
    "@angular-eslint/schematics": "~18.4.3",
    "@angular-eslint/template-parser": "~18.4.3",
    "@angular/cli": "^17.3.11",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/language-service": "^17.3.12",
    "@ngneat/until-destroy": "^9.0.0",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.20.37",
    "@typescript-eslint/eslint-plugin": "~5.6.0",
    "@typescript-eslint/parser": "~5.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "htmlhint": "^1.1.3",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "postcss": "^8.5.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "~14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "~5.0.2",
    "stylelint-config-standard": "~24.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.1",
    "typescript": "~5.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}